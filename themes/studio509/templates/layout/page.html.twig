<div id="page">
    <header id="header" role="banner" aria-label="{{ 'Site header'|t }}" class="wrapper clearfix">
      <div class="container">
        <nav class="navbar navbar-expand-lg navbar-light">
					{{ page.header }}
					
					<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="navbarNavDropdown">
					{{ page.navbar_menu }}
					{{ page.navbar_search }}
					</div>
				</nav>
      </div>
    </header>
		
		
		{{ page.primary_menu }}

	
		{% if page.slider %}
      <div id="slider-wrapper" class="wrapper">
        {{ page.slider }}
      </div>
    {% endif %}
	
    {% if page.highlighted %}
	  <div class="container">
      <div class="highlighted">
        <aside class="layout-container section clearfix" role="complementary">
          {{ page.highlighted }}
        </aside>
      </div>
	  </div>
    {% endif %}
    {% if page.featured_top %}
	  <div class="container">
      <div class="featured-top">
        <aside class="featured-top__inner section layout-container clearfix" role="complementary">
          {{ page.featured_top }}
        </aside>
      </div>
	  </div>
    {% endif %}
    <div id="main-wrapper" class="wrapper">
      <div id="main" class="container">
        {{ page.breadcrumb }}
        <main id="content" class="column main-content" role="main">
          <section class="section">
            <a id="main-content" tabindex="-1"></a>
            {{ page.content }}
          </section>
        </main>
        {% if page.sidebar_first %}
          <div id="sidebar-first" class="column sidebar">
            <aside class="section" role="complementary">
              {{ page.sidebar_first }}
            </aside>
          </div>
        {% endif %}
        {% if page.sidebar_second %}
          <div id="sidebar-second" class="column sidebar">
            <aside class="section" role="complementary">
              {{ page.sidebar_second }}
            </aside>
          </div>
        {% endif %}
      </div>
    </div>
	
    {% if page.featured_bottom_first or page.featured_bottom_second or page.featured_bottom_third %}
	<div id="triptych" class="wrapper">
      <div class="container">
        <aside class="row">
		  {% if page.featured_bottom_first %}
		  <div class="col-xs-12 col-sm-6 col-md-4 mb16">
            {{ page.featured_bottom_first }}
		  </div>
		  {% endif %}
		  {% if page.featured_bottom_second %}
		  <div class="col-xs-12 col-sm-6 col-md-4 mb16">
            {{ page.featured_bottom_second }}
		  </div>
		  {% endif %}
		  {% if page.featured_bottom_third %}
	      <div class="col-xs-12 col-sm-12 col-md-4 mb16">
            {{ page.featured_bottom_third }}
		  </div>
		  {% endif %}
        </aside>
      </div>
	</div>
    {% endif %}
	
    <footer id="footer" class="wrapper mb32">
	  {% if page.footer_first or page.footer_second or page.footer_third or page.footer_fourth %}
      <div class="container">
        
          <div class="site-footer_top row">
		  	{% if page.footer_first %}
		  	<div class="col-xs-12 col-sm-6 col-md-3 mb16">
            {{ page.footer_first }}
			</div>
			{% endif %}
			{% if page.footer_second %}
			<div class="col-xs-12 col-sm-6 col-md-3 mb16">
            {{ page.footer_second }}
			</div>
			{% endif %}
			{% if page.footer_third %}
			<div class="col-xs-12 col-sm-6 col-md-3 mb16">
            {{ page.footer_third }}
			</div>
			{% endif %}
			{% if page.footer_fourth %}
			<div class="col-xs-12 col-sm-6 col-md-3 mb16">
            {{ page.footer_fourth }}
			</div>
			{% endif %}
          </div>
        
	 </div>
	 {% endif %}
	 
	 <div class="container">
        {% if page.footer_fifth %}
          <div class="site-footer_bottom row">
		  	<div class="col-xs-12">	
            {{ page.footer_fifth }}
			</div>
          </div>
        {% endif %}
      </div>
    </footer>
  </div>
